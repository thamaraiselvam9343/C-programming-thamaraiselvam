#include <stdio.h>
int main() {
    int N, original, reverse = 0;
    scanf("%d", &N);
    original = N;
    while (N > 0) {
        int digit = N % 10;
        reverse = reverse * 10 + digit;
        N = N / 10;
    }
    if (original == reverse)
        printf("Palindrome Number");
    else
        printf("Not a Palindrome Number");
}
