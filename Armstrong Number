#include <stdio.h>
#include <math.h>
int main() {
    int N, temp, digits = 0;
    int sum = 0;
    scanf("%d", &N);
    temp = N;
    while(temp > 0) {
        digits++;
        temp /= 10;
    }
    temp = N;
    while(temp > 0) {
        int digit = temp % 10;
        sum += pow(digit, digits);
        temp /= 10;
    }
    if(sum == N)
        printf("Armstrong Number");
    else
        printf("Not an Armstrong Number");
}
